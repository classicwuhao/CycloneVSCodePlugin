{
  "name": "Cyclone",
  "publisher": "HaoWu",
  "displayName": "Cyclone",
  "description": "Cyclone Plugin for Visual Studio Code",
  "icon": "icon.png",
  "version": "0.0.8",
  "public": false,
  "homepage": "https://github.com/classicwuhao/CycloneVSCodePlugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/classicwuhao/CycloneVSCodePlugin"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "activationEvents": [],
  "main": "./extension.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cyclone",
        "aliases": [
          "cyclone",
          "cyclone"
        ],
        "extensions": [
          ".cyclone"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cyclone",
        "scopeName": "text.cyclone",
        "path": "./syntaxes/cyclone.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "cyclone.m1.check",
        "title": "Check This Cyclone Spec",
        "category": "Cyclone"
      },
      {
        "command": "cyclone.m2.trace",
        "title": "Show Trace File",
        "category": "Cyclone"
      },
      {
        "command": "cyclone.m3.clean",
        "title": "Clean Trace File",
        "category": "Cyclone"
      },
      {
        "command": "cyclone.m4.cleanAll",
        "title": "Clean All Trace Files",
        "category": "Cyclone"
      },
      {
        "command": "cyclone.m5.version",
        "title": "Version Info",
        "category": "Cyclone"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "Cyclone.Submenu",
          "group": "Cyclone.Menu"
        }
      ],
      "Cyclone.Submenu": [
        {
          "command": "cyclone.m1.check",
          "group": "Cyclone.MainItem",
          "when": "resourceExtname == .cyclone"
        },
        {
          "command": "cyclone.m2.trace",
          "group": "Cyclone.MainItem",
          "when": "resourceExtname == .cyclone"
        },
        {
          "command": "cyclone.m3.clean",
          "group": "Cyclone.MainItem",
          "when": "resourceExtname == .cyclone"
        },
        {
          "command": "cyclone.m4.cleanAll",
          "group": "Cyclone.MainItem",
          "when": "resourceExtname == .cyclone"
        },
        {
          "command": "cyclone.m5.version",
          "group": "Cyclone.MenuInfo"
        }
      ]
    },
    "submenus": [
      {
        "label": "Cyclone",
        "id": "Cyclone.Submenu"
      }
    ]
  },
  "devDependencies": {
    "vscode": "^1.1.37"
  }
}
