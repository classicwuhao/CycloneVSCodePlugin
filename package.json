{
  "name": "Cyclone",
  "publisher": "HaoWu",
  "displayName": "Cyclone",
  "description": "Cyclone Plugin for Visual Studio Code",
  "icon": "icon.png",
  "version": "0.0.8",
  "public": false,
  "homepage": "https://github.com/classicwuhao/CycloneVSCodePlugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/classicwuhao/CycloneVSCodePlugin"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "activationEvents": [
  ],
  "main": "./extension.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cyclone",
        "aliases": [
          "cyclone",
          "cyclone"
        ],
        "extensions": [
          ".cyclone"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cyclone",
        "scopeName": "text.cyclone",
        "path": "./syntaxes/cyclone.tmLanguage"
      }
    ],
    "commands": [{
      "command": "cyclone.check",
      "title": "Check This Cyclone Spec",
      "category": "Cyclone"
    },
    {
      "command": "cyclone.trace",
      "title": "Open Trace File",
      "category": "Cyclone"
    },
    {
      "command" : "cyclone.version",
      "title" : "Version Info",
      "category" : "Cyclone"
    }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu" : "Cyclone.Submenu",
          "group" : "Cyclone.Menu"
        }
      ],
      "Cyclone.Submenu" : [
        {
          "command" : "cyclone.check",
          "group" : "Cyclone.CheckMenu",
          "when": "resourceExtname == .cyclone"
        },
        {
          "group" : "Cyclone.InfoMenu",
          "command" : "cyclone.version"
        }
      ]
    },
    "submenus" : [{
      "label" : "Cyclone",
      "id" : "Cyclone.Submenu"
    }
  ]
  },
  "devDependencies": {
    "vscode": "^1.1.37"
  }
}